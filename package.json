    "@nuxtjs/tailwindcss": "^6.4.1",
    "@pinia-plugin-persistedstate/nuxt": "^1.1.1",
    "@vueuse/core": "^9.13.0",
    "@vueuse/nuxt": "^9.13.0",
    "nuxt": "^3.2.3",
    "nuxt-headlessui": "^1.1.1",
    "nuxt-icon": "^0.3.2",
    "nuxt-schema-org": "^2.1.2"
  },
  "dependencies": {
