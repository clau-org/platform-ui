{
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "build:deno": "NITRO_PRESET=deno nuxi build",
    "build:aws-lambda": "NITRO_PRESET=aws-lambda nuxi build",
    "preview:deno": "cd .output/ && deno run -A server/index.ts",
    "deploy:deno": "cd .output/ && deployctl deploy --project='clau-platform' --prod server/index.ts && cd ..",
    "deploy:aws-lambda": "sls deploy --force",
    "build:preview:deno": "npm run build:deno && npm run preview:deno",
    "build:deploy:deno": "npm run build:deno && npm run deploy:deno",
    "build:deploy:aws-lambda": "npm run build:aws-lambda && npm run deploy:aws-lambda",
    "build:deploy": "npm run build:deploy:aws-lambda && npm run build:deploy:deno"
  },
  "devDependencies": {
    "@nuxt/image-edge": "^1.0.0-27968280.9739e4d",
